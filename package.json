{
  "name": "playwright-scraper",
  "version": "1.0.0",
  "description": "A scraper for extracting job listings from a specified URL using Playwright.",
  "main": "src/scraper.ts",
  "scripts": {
    "start": "ts-node src/scraper.ts",
    "test": "jest",
    "test:coverage": "jest --coverage",
    "lint": "eslint \"src/**/*.ts\"",
    "lint:fix": "eslint \"src/**/*.ts\" --fix",
    "format": "prettier --write \"src/**/*.{ts,json,md}\"",
    "format:check": "prettier --check \"src/**/*.{ts,json,md}\"",
    "type-check": "tsc --noEmit",
    "build": "tsc",
    "dev": "ts-node-dev --respawn src/scraper.ts",
    "scrape": "ts-node src/index.ts"
  },
  "dependencies": {
    "@supabase/supabase-js": "^2.49.8",
    "@types/papaparse": "^5.3.15",
    "date-fns": "^4.1.0",
    "dotenv": "^16.5.0",
    "papaparse": "^5.5.2",
    "playwright": "^1.52.0",
    "winston": "^3.17.0"
  },
  "devDependencies": {
    "@playwright/test": "^1.52.0",
    "@types/jest": "^27.5.2",
    "@types/node": "^16.18.126",
    "@typescript-eslint/eslint-plugin": "^8.30.1",
    "@typescript-eslint/parser": "^8.30.1",
    "eslint": "^9.24.0",
    "eslint-config-prettier": "^10.1.2",
    "globals": "^16.0.0",
    "jest": "^27.0.0",
    "prettier": "^3.5.3",
    "ts-jest": "^27.1.5",
    "ts-node": "^10.9.2",
    "typescript": "^4.9.5"
  },
  "keywords": [
    "scraper",
    "playwright",
    "web scraping",
    "jobs"
  ],
  "author": "Your Name",
  "license": "MIT"
}
